{
  "name": "h5web",
  "private": true,
  "version": "1.3.0",
  "description": "Web-based HDF5 file viewer",
  "author": "European Synchrotron Radiation Facility",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silx-kit/h5web"
  },
  "engines": {
    "node": "16.x",
    "pnpm": "*"
  },
  "scripts": {
    "start": "pnpm start --filter demo",
    "start:storybook": "pnpm start --filter storybook",
    "build": "pnpm build --filter demo",
    "build:storybook": "pnpm build --filter storybook",
    "serve": "pnpm dlx serve -s -p 3000 apps/demo/build",
    "serve:storybook": "pnpm dlx http-server -p 6006 apps/storybook/build",
    "packages": "pnpm build --filter {packages}",
    "packages:dts": "pnpm build:dts --filter {packages}",
    "lint": "eslint \"**/*.{js,cjs,ts,tsx}\" --max-warnings=0",
    "lint:all": "pnpm lint -r --parallel",
    "prettier": "prettier . --check",
    "test": "jest",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.2",
    "@types/cypress-image-snapshot": "3.1.6",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "cypress": "9.1.1",
    "cypress-image-snapshot": "4.0.1",
    "eslint": "8.4.1",
    "eslint-config-galex": "3.3.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.3",
    "jest-transform-stub": "2.0.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.0",
    "typescript": "4.5.2"
  },
  "pnpm": {
    "overrides": {
      "react-scripts>eslint-plugin-testing-library": ">=5",
      "react-scripts>@typescript-eslint/parser": ">=5",
      "react-scripts>@typescript-eslint/eslint-plugin": ">=5"
    }
  }
}
