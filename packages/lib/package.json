{
  "name": "@h5web/lib",
  "version": "0.0.1",
  "description": "Data visualization toolkit",
  "author": "European Synchrotron Radiation Facility",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silx-kit/h5web"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "build:dts": "tsc --build tsconfig.build.json && rollup -c",
    "lint:eslint": "eslint \"**/*.{js,cjs,ts,tsx}\" --max-warnings=0",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "analyze": "pnpm dlx source-map-explorer dist/index.js --no-border-checks"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=6.0.14",
    "react": ">=16",
    "react-dom": ">=16",
    "three": ">=0.120"
  },
  "dependencies": {
    "@react-hookz/web": "12.0.0",
    "@visx/axis": "2.4.0",
    "@visx/grid": "2.4.0",
    "@visx/scale": "2.2.2",
    "@visx/shape": "2.4.0",
    "@visx/tooltip": "2.2.2",
    "d3-array": "3.1.1",
    "d3-color": "3.0.1",
    "d3-format": "3.1.0",
    "d3-interpolate": "3.0.1",
    "d3-scale": "4.0.2",
    "d3-scale-chromatic": "3.0.0",
    "lodash": "4.17.21",
    "ndarray": "1.0.19",
    "ndarray-ops": "1.2.2",
    "react-aria-menubutton": "7.0.3",
    "react-icons": "4.3.1",
    "react-keyed-flatten-children": "1.3.0",
    "react-measure": "2.5.2",
    "react-slider": "1.3.1",
    "react-use": "17.3.1",
    "react-window": "1.8.6"
  },
  "devDependencies": {
    "@h5web/shared": "workspace:*",
    "@react-three/fiber": "7.0.24",
    "@types/d3-array": "3.0.2",
    "@types/d3-color": "3.0.2",
    "@types/d3-format": "3.0.1",
    "@types/d3-interpolate": "3.0.1",
    "@types/d3-scale": "4.0.2",
    "@types/d3-scale-chromatic": "3.0.0",
    "@types/lodash": "4.14.177",
    "@types/ndarray": "1.0.11",
    "@types/react": "17.0.37",
    "@types/react-aria-menubutton": "6.2.8",
    "@types/react-dom": "17.0.11",
    "@types/react-measure": "2.0.8",
    "@types/react-slider": "1.3.1",
    "@types/react-window": "1.8.5",
    "@types/three": "0.135.0",
    "eslint": "8.4.1",
    "eslint-config-galex": "3.3.6",
    "jest": "27.4.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.60.2",
    "rollup-plugin-dts": "4.0.1",
    "three": "0.135.0",
    "typescript": "4.5.2",
    "vite": "2.7.1"
  }
}
